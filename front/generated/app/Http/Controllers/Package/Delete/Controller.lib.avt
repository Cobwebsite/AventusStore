import { Response } from './Response.lib.avt';

namespace App.Http.Controllers.Package.Delete {

	export class PackageDeleteController extends Aventus.HttpRoute {
		@BindThis()
		public async request(name: string | number): Promise<Aventus.ResultWithError<Response>> {
			const request = new Aventus.HttpRequest(`${this.getPrefix()}/api/package/${name}`, Aventus.HttpMethod.DELETE);
			return await request.queryJSON<Response>(this.router);
		}
	}

}