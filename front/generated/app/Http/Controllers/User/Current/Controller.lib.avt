import { UserDetailsResource } from '../UserDetailsResource.lib.avt';

namespace App.Http.Controllers.User.Current {

	export class UserCurrentController extends Aventus.HttpRoute {
		@BindThis()
		public async request(): Promise<Aventus.ResultWithError<UserDetailsResource>> {
			const request = new Aventus.HttpRequest(`${this.getPrefix()}/api/user/current`, Aventus.HttpMethod.GET);
			return await request.queryJSON<UserDetailsResource>(this.router);
		}
	}

}